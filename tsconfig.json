{
    "compilerOptions": {
        "target": "ES2022",
        "allowJs": true,
        "module": "CommonJS",
        "sourceMap": true,
        "strict": false,
        "forceConsistentCasingInFileNames": true,
        "noEmit": false,
        "incremental": true,
        "moduleResolution": "node",
        "esModuleInterop": false
    },
    "exclude": [
        "node_modules"
    ],
    "include": [
        "**/*.ts"
    ]
}
